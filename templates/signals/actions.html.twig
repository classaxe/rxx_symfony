{% if (signals|length > 0 or isAdmin) %}
<p class="report_actions">
{% if signals|length > 0 %}
    <input type="button" id="btn_prt" class="button small" value="{% trans %}Print...{% endtrans %}" />
    <input type="button" id="btn_csv" class="button small" value="{{ 'Export as %s' |trans|format('.csv')}}" />
{#    <input type="button" id="btn_txt" class="button small" value="{{ 'Export as %s' |trans|format('.txt')}}" />#}
{#    <input type="button" id="btn_kml" class="button small" value="{{ 'Export as %s' |trans|format('.kml')}}" />#}
{% endif %}
{% if isAdmin %}
    {% if isAdmin %}
        <input type="button" class="button small" value="{{ 'Add Signal...' | trans }}"
               onclick="window.open('{{ path('signal', { 'system' : system, 'id' : 'new'})}}', 'signal_new', popWinSpecs['signals_[id]'])"
        >
    {% endif %}
{% endif %}
</p>
{% endif %}