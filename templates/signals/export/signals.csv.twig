    s.formattedLsbRel }}","{{
    s.formattedUsbRel }}","{{
    s.sec }}","{{
    s.getFormat }}","{{
    s.qth|raw }}","{{
    s.sp }}","{{
    s.itu }}","{{
    s.region }}","{{
    s.gsq }}","{{
    s.lat }}","{{
    s.lon }}","{{
    s.pwr ? s.pwr : '' }}","{{
    s.notes }}","{{
    s.heardIn }}","{{
    s.logs }}","{{
    s.firstHeard|date('Y-m-d') }}","{{
    s.lastHeard|date('Y-m-d') }}"{% if args.personalise %},"{{ s.range_km }}","{{ s.range_mi }}","{{ s.range_deg }}"{% endif %}

