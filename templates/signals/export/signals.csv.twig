"{%
    trans %}KHz{% endtrans %}","{%
    trans %}ID{% endtrans %}","{%
    trans %}Type{% endtrans %}","{%
    trans %}Active{% endtrans %}","{%
    trans %}LSB{% endtrans %}","{%
    trans %}USB{% endtrans %}","{%
    trans %}Sec{% endtrans %}","{%
    trans %}Fmt{% endtrans %}","{%
    trans %}'Name' and Location'{% endtrans %}","{%
    trans %}SP{% endtrans %}","{%
    trans %}ITU{% endtrans %}","{%
    trans %}Region{% endtrans %}","{%
    trans %}GSQ{% endtrans %}","{%
    trans %}Lat{% endtrans %}","{%
    trans %}Lon{% endtrans %}","{%
    trans %}Pwr{% endtrans %}","{%
    trans %}Notes{% endtrans %}","{%
    trans %}Heard In{% endtrans %}","{%
    trans %}Logs{% endtrans %}","{%
    trans %}First Logged{% endtrans %}","{%
    trans %}Last Logged{% endtrans
%}"
{% for s in signals %}{{
    0 + s.khz }},"{{
    s.call }}","{{
    typeRepository.typeForCode(s.type)['label']|trans }}","{{
    s.active ? 'Y' : 'N' }}","{{
    s.formattedLsbRel }}","{{
    s.formattedUsbRel }}","{{
    s.sec }}","{{
    s.getFormat }}","{{
    s.qth|raw }}","{{
    s.sp }}","{{
    s.itu }}","{{
    s.region }}","{{
    s.gsq }}","{{
    s.lat }}","{{
    s.lon }}","{{
    s.pwr ? s.pwr : '' }}","{{
    s.notes }}","{{
    s.heardIn }}","{{
    s.logs }}","{{
    s.firstHeard|date('Y-m-d') }}","{{
    s.lastHeard|date('Y-m-d') }}"
{% endfor %}