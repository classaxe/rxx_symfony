{% for l in logs %}"{{ l.receiver }}","{{
    l.logDate }}","{{
    l.logTime|slice(0,2) }}:{{ l.logTime|slice(2,2) }}","{{
    l.khz }}","{{
    l.call }}","{{
    typeRepository.typeForCode(l.type)['label']|trans }}","{{
    l.active ? 'Y' : 'N' }}","{{
    l.qth|raw }}","{{
    l.sp }}","{{
    l.itu }}","{{
    l.region }}","{{
    l.gsq }}","{{
    l.lat }}","{{
    l.lon }}","{{
    l.lsb }}","{{
    l.usb }}","{{
    l.sec }}","{{
    l.format }}","{{
    l.pwr ? l.pwr : '' }}","{{
    l.dxKm }}","{{
    l.dxMiles }}","{{
    (l.daytime ? 'Y' : 'N') }}"
{% endfor %}