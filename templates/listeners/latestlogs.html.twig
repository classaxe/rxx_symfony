{%  if isAdmin %}
    <div class="links_left row addLogFor">
        <div class="small-12 medium-1"><strong>{{ "Latest Logs:" | trans }}</strong></div>
        <div class="small-12 medium-11">
            {% for l in latestLogs['listeners'] %}
                <a href="{{ url('listener_logs', { 'system' : system, 'id': l.id }) }}"
                   title="{{ l.id }} | {{ l.name|raw}}, {{ l.qth }}{% if l.sp %}, {{ l.sp }}{% endif %}, {{ l.itu }} [{{ l.gsq }}]"
                   data-popup="1"{% if l.primaryQth == 'Y' %} class="pri"{% endif %}>
                    {{ l.name|raw }}
                </a> ({{ l.sp ? l.sp : l.itu }})
                {% if not loop.last %} &nbsp; | &nbsp; {% endif %}
            {% endfor %} &nbsp; <strong>({{ latestLogs['date'] }})</strong>
        </div>
    </div>
{% endif %}
