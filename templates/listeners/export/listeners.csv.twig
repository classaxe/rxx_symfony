"{%
    trans %}RXX ID{% endtrans %}","{%
    trans %}Active{% endtrans %}","{%
    trans %}Primary{% endtrans %}","{%
    trans %}Multi-Op{% endtrans %}","{%
    trans %}Name{% endtrans %}","{%
    trans %}Callsign{% endtrans %}","{%
    trans %}QTH{% endtrans %}","{%
    trans %}SP{% endtrans %}","{%
    trans %}ITU{% endtrans %}","{%
    trans %}Region{% endtrans %}","{%
    trans %}GSQ{% endtrans %}","{%
    trans %}Lat{% endtrans %}","{%
    trans %}Lon{% endtrans %}","{%
    trans %}Timezone{% endtrans %}","{%
    trans %}Equipment{% endtrans %}","{% if isAdmin %}{%
    trans %}Notes{% endtrans %}","{% endif %}{%
    trans %}Latest Upload{% endtrans %}","{%
    trans %}Earliest Log{% endtrans %}","{%
    trans %}Latest Log{% endtrans %}","{%
    trans %}Log Sessions{% endtrans %}","{%
    trans %}Total Logs{% endtrans %}","{%
    trans %}DGPS{% endtrans %}","{%
    trans %}DSC{% endtrans %}","{%
    trans %}HAMBCN{% endtrans %}","{%
    trans %}NAVTEX{% endtrans %}","{%
    trans %}NDB{% endtrans %}","{%
    trans %}TIME{% endtrans %}","{%
    trans %}OTHER{% endtrans %}","{%
    trans %}All Signals{% endtrans %}","{%
    trans %}Remote Sessions{% endtrans %}","{%
